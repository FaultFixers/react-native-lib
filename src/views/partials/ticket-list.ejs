<% if (tickets.length) { %>
    <ul>
        <% tickets.forEach(ticket => { %>
            <li><a href="/tickets/<%= ticket.friendlyId %>"><%= ticket.friendlyId %></a></li>
        <% }) %>
    </ul>
<% } else if (showNoTicketsMessage) { %>
    <p class="no-tickets-message"><%= showNoTicketsMessage %></p>
<% } %>
